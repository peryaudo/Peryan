extern mes :: String -> Void

extern printNum :: Int -> Void

extern strlen_ :: String -> Int // will be rewritten natively

func strlen(str :: String) :: Int {
	return strlen_(str)
}

func str(x :: Int) :: String {
	return String(x)
}

