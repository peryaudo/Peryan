#ifndef TOKEN
#define TOKEN(X)
#endif

#ifndef PUNCTUATOR
#define PUNCTUATOR(X,Y) TOKEN(X)
#endif

#ifndef KEYWORD
#define KEYWORD(X,Y) TOKEN(KW_ ## X)
#endif

// Tokens
TOKEN(UNKNOWN)
TOKEN(ID)
TOKEN(TYPE_ID)
TOKEN(INTEGER)
TOKEN(FLOAT)
TOKEN(STRING)
TOKEN(CHAR)
TOKEN(TERM)
TOKEN(END)

// Punctuators
PUNCTUATOR(DOT, ".")
PUNCTUATOR(COMMA, ",")
PUNCTUATOR(CLN, ":")
PUNCTUATOR(CLNCLN, "::")
PUNCTUATOR(LBRACE, "{")
PUNCTUATOR(RBRACE, "}")
PUNCTUATOR(LBRACK, "[")
PUNCTUATOR(RBRACK, "]")
PUNCTUATOR(LPAREN, "(")
PUNCTUATOR(RPAREN, ")")

PUNCTUATOR(EQL, "=")
PUNCTUATOR(EQEQ, "==")
PUNCTUATOR(EXCL, "!")
PUNCTUATOR(EXCLEQ, "!=")
PUNCTUATOR(LT, "<")
PUNCTUATOR(LTEQ, "<=")
PUNCTUATOR(GT, ">")
PUNCTUATOR(GTEQ, ">=")

PUNCTUATOR(RARROW, "->")

PUNCTUATOR(PLUSPLUS, "++")
PUNCTUATOR(MINUSMINUS, "--")
PUNCTUATOR(PLUSEQ, "+=")
PUNCTUATOR(MINUSEQ, "-=")
PUNCTUATOR(STAREQ, "*=")
PUNCTUATOR(SLASHEQ, "/=")

PUNCTUATOR(PLUS, "+")
PUNCTUATOR(MINUS, "-")
PUNCTUATOR(STAR, "*")
PUNCTUATOR(SLASH, "/")
PUNCTUATOR(PERC, "%")
PUNCTUATOR(AMP, "&")
PUNCTUATOR(PIPE, "|")
PUNCTUATOR(CARET, "^")
PUNCTUATOR(LTLT, "<<")
PUNCTUATOR(GTGT, ">>")

// Keywords
KEYWORD(TRUE, "true")
KEYWORD(FALSE, "false")

KEYWORD(FUNC, "func")
KEYWORD(MODULE, "module")
KEYWORD(PUBLIC, "public")
KEYWORD(PRIVATE, "private")
KEYWORD(VAR, "var")
KEYWORD(CONST, "const")
KEYWORD(REF, "ref")
KEYWORD(THIS, "this")

KEYWORD(IF, "if")
KEYWORD(ELSE, "else")
KEYWORD(REPEAT, "repeat")
KEYWORD(LOOP, "loop")
KEYWORD(WHILE, "while")
KEYWORD(WEND, "wend")
KEYWORD(DO, "do")
KEYWORD(UNTIL, "until")
KEYWORD(FOR, "for")
KEYWORD(NEXT, "next")
KEYWORD(SWITCH, "switch")

KEYWORD(CONTINUE, "continue")

KEYWORD(BREAK, "break")
KEYWORD(SWBREAK, "swbreak")

KEYWORD(GOTO, "goto")
KEYWORD(GOSUB, "gosub")

KEYWORD(RETURN, "return")

KEYWORD(EXTERN, "extern")

KEYWORD(PARTIAL, "partial")

#undef TOKEN
#undef NONTOKEN
#undef PUNCTUATOR
#undef KEYWORD
